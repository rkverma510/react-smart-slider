{
  "name": "react-smart-slider",
  "version": "0.1.0",
  "description": "Super light-weight, easy to use React JS smart slider",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx"
    ]
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.7.0"
  },
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest",
    "start": "webpack --watch",
    "build": "webpack-cli --mode production"
  },
  "keywords": [
    "react",
    "slider",
    "gallery",
    "slideshow",
    "react-smart-slider",
    "react-slider",
    "react-gallery",
    "react-slideshow",
    "image slider",
    "image gallery",
    "animated slider"
  ],
  "author": {
    "name": "Rajesh kumar",
    "email": "rkverma510@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therkverma/react-smart-slider.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/therkverma/react-smart-slider/issues"
  },
  "homepage": "https://github.com/therkverma/react-smart-slider#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
