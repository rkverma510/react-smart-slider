{
  "name": "react-smart-slider",
  "version": "0.1.3",
  "description": "Super light-weight, easy to use React JS smart slider",
  "main": "build/index.js",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "transform-react-jsx"
    ]
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest",
    "start": "webpack --watch --mode production",
    "build": "webpack-cli --mode production"
  },
  "keywords": [
    "react",
    "slider",
    "gallery",
    "slideshow",
    "react-smart-slider",
    "react-slider",
    "react-gallery",
    "react-slideshow",
    "image slider",
    "image gallery",
    "animated slider"
  ],
  "author": {
    "name": "Rajesh kumar",
    "email": "rkverma510@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therkverma/react-smart-slider.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/therkverma/react-smart-slider/issues"
  },
  "homepage": "https://github.com/therkverma/react-smart-slider#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
